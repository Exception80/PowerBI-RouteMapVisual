{
    "dataRoles": [
        {
            "displayName": "Market",
            "name": "market",
            "kind": "Grouping"
        },	
		{
            "displayName": "Location From",
            "name": "codeFrom",
            "kind": "Grouping"
        },
		{
            "displayName": "Latitude From",
            "name": "latFrom",
            "kind": "Measure"
        },
		{
            "displayName": "Longitude From",
            "name": "longFrom",
            "kind": "Measure"
        },
		{
            "displayName": "Location To",
            "name": "codeTo",
            "kind": "Grouping"
        },	
		{
            "displayName": "Latitude To",
            "name": "latTo",
            "kind": "Measure"
        },
		{
            "displayName": "Longitude To",
            "name": "longTo",
            "kind": "Measure"
        },
        {
            "displayName": "State value",
            "name": "stateValue",
            "kind": "Measure"
        },
        {
            "displayName": "Tooltips",
            "name": "tooltips",
            "kind": "Measure"
        }
    ],
    "dataViewMappings": [
		{
			"conditions": [
				{
                    "market": {
                        "max": 1
                    },
					"codeFrom": {
                        "max": 1
                    },
					"latFrom": {
                        "max": 1
                    },
					"longFrom": {
                        "max": 1
                    },					
					"codeTo": {
                        "max": 1
                    },
					"latTo": {
                        "max": 1
                    },
					"longTo": {
                        "max": 1
                    },
                    "stateValue": {
                        "max": 1 
                    },
                    "tooltips": { 
                        "max": 0 
                    } 
				},
                {
                    "market": {
                        "min": 1,
                        "max": 1
                    },
					"codeFrom": {
                        "min": 1,
                        "max": 1
                    },
					"latFrom": {
                        "min": 1,
                        "max": 1
                    },
					"longFrom": {
                        "min": 1,
                        "max": 1
                    },					
					"codeTo": {
                        "min": 1,
                        "max": 1
                    },
					"latTo": {
                        "min": 1,
                        "max": 1
                    },
					"longTo": {
                        "min": 1,
                        "max": 1
                    },
                    "stateValue": {
                        "min": 0,
                        "max": 1
                    }
				}
			],
            "categorical": {
                "categories": {
                    "select": [
                        { "bind": { "to": "market" } },
                        { "bind": { "to": "codeFrom" } },
                        { "bind": { "to": "codeTo" } }
                    ],
                    "dataReductionAlgorithm": {
                        "top": {}
                    }
                },
				"values": {
                    "group": {
                        "by": "codeTo",
                        "select":[
                            { "bind": { "to": "latFrom" } },
                            { "bind": { "to": "longFrom" } },
                            { "bind": { "to": "latTo" } },
                            { "bind": { "to": "longTo" } },
                            { "bind": { "to": "stateValue" } },
                            { "for": { "in": "tooltips" } }
                        ]
                    }
                }                
			}
        }                 					
    ],
    "objects": {
        "routes": {
            "displayName": "Routes",
            "properties": {
                "arcColor": {
                    "displayName": "Arc color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }                
            }
        },
        "markers": {
            "displayName": "Markers",
            "properties": {
                "markerColor": {
                    "displayName": "Marker color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }  
                },
                "radius": {
                    "displayName": "Radius",
                    "type": {
                        "numeric": true
                    }
                },
                "labelFontColor": {
                    "displayName": "Label color",
                    "type": {
                        "fill": {
                            "solid": {
                                "color": true
                            }
                        }
                    }
                }
            }            
        },
        "state1": {
            "displayName": "State 1",
            "properties": {
                "stateColor": {
                    "displayName": "Color",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "dataMin": {
                    "displayName": "From value",
                    "type": { "numeric": true }
                },
                "dataMax": {
                    "displayName": "To value",
                    "type": { "numeric": true }
                }
            }
        },
        "state2": {
            "displayName": "State 2",
            "properties": {
                "stateColor": {
                    "displayName": "Color",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "dataMin": {
                    "displayName": "From value",
                    "type": { "numeric": true }
                },
                "dataMax": {
                    "displayName": "To value",
                    "type": { "numeric": true }
                }
            }
        },
        "state3": {
            "displayName": "State 3",
            "properties": {
                "stateColor": {
                    "displayName": "Color",
                    "type": { "fill": { "solid": { "color": true } } }
                },
                "dataMin": {
                    "displayName": "From value",
                    "type": { "numeric": true }
                },
                "dataMax": {
                    "displayName": "To value",
                    "type": { "numeric": true }
                }
            }
        }       
    }
}